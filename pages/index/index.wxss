/**index.wxss**/
.container {
  /* position: relative; */
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding-bottom: 200rpx;
  box-sizing: border-box;
}

.bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: calc(100vh);
  z-index: -1;
}

.inner {
  position: relative;
  top: 400rpx;
  width: 573rpx;
  height: 660rpx;
  margin: auto;
}
.relative{
  position: relative;
}
.part1 .partition .topPart{
  position: absolute;
  z-index: -10;
  top: -50rpx;
  left: 15rpx;
  width: 80rpx;
  height: 45rpx;
  background-color: #2d5278;
  border: solid 6rpx rgb(126,206,244);
  border-radius: 50% 50% 0 0;
  border-width:6rpx 6rpx 0rpx 6rpx;
  /* opacity: 0.8; */
}


.partition {
  float: left;
  margin:20rpx 0rpx 20rpx 17rpx;
  width: 257rpx;
  height: 241rpx;
  background-color: #2d5278;
  box-shadow: 0rpx 3rpx 7rpx 0rpx rgba(0, 0, 0, 0.35);
  border: solid 6rpx rgb(126,206,244);
  border-radius: 20rpx;
  opacity: 0.8;
}

